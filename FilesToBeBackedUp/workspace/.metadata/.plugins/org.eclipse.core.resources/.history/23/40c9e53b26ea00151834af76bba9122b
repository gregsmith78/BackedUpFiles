import java.util.*;
public class GoPalindrome {

	public static void main(String[] args) {
		//StringBuilder text = new StringBuilder();
	    String text = "racecar";
		String cutText;
		cutText = text.substring(0,3);
		
		
	    for (char i = 0; i < cutText.length(); i++){
		System.out.println(i);
	}
	}	
}
