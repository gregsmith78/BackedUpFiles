import java.util.*;
public class GoPalindrome {

	public static void main(String[] args) {
		//StringBuilder text = new StringBuilder();
	    String text = "racecar";
		String cutText;
		cutText = text.substring(0,3);
	
		
		
	    for (int i = 0; i < cutText.lastIndexOf(6); i++){
		System.out.println(i);
		
		
	}
	}	
}
