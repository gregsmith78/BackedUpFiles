import java.util.*;

public class GoArrayLists {
	 static boolean retreval1;
	boolean retraval2;

	public static void main(String[] args) {
		ArrayList<Integer> firstArray = new ArrayList<Integer>();
		ArrayList<Integer> secondArray = new ArrayList<Integer>();
		
		firstArray.add(21);
		firstArray.add(11);
		firstArray.add(34);
		firstArray.add(6);
		
		retreval1 = firstArray.containsAll(secondArray);
		for(Integer item: firstArray){
			System.out.println(item);
		}

		secondArray.add(11);
		secondArray.add(21);
		secondArray.add(22);
		secondArray.add(66);
		secondArray.add(36);
		for(Integer item: secondArray){
			System.out.println(item);
		
		}
		if (retreval1 == true){
			System.out.println("Array1 has all the elements of Array2.");
		} else {
			System.out.println("These arrays do not match up.");
		}
		

	}

}
