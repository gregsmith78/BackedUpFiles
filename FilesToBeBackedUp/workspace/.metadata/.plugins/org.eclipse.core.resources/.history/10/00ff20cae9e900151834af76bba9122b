import java.util.*;
public class Being {
	String name;
	int speed;
	int power;
	int pass;
	int tackle;
	int strength;
	
	
	public Being(String Name, int Speed, int Power, int Pass, int Tackle, int Strength){
		name = Name;
		speed = Speed;
		power = Power;
		pass = Pass;
		tackle = Tackle;
		strength = Strength;
		
	}
	

}
